	<parameter name="dataFimFormat" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT dia,
       total_entradas,
       total_saidas,
       (total_entradas - total_saidas)                                                                       AS saldo_do_dia,
       SUM(total_entradas - total_saidas) OVER (ORDER BY dia) + (select c.valor_abertura
                                                                 from caixa c
                                                                 where c.caixa_id = totais_diarios.caixa_id) AS saldo_acumulado,
       descricao
FROM (SELECT CAST(data_movimentacao AS DATE)                                                    as dia,
             COALESCE(SUM(CASE WHEN mc.tipo_movimentacao = 'ENTRADA' THEN valor ELSE 0 END), 0) as total_entradas,
             COALESCE(SUM(CASE WHEN mc.tipo_movimentacao = 'SAIDA' THEN valor ELSE 0 END), 0)   as total_saidas,
             mc.caixa_id,
             mc.descricao                                                                       as descricao
      FROM movimentacao_caixa mc
      WHERE CAST(data_movimentacao AS DATE) BETWEEN   $P{dataInicio} AND  $P{dataFim} 
      GROUP BY CAST(data_movimentacao AS DATE), mc.caixa_id, mc.descricao) AS totais_diarios
ORDER BY dia ASC;]]></query>

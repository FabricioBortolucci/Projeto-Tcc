	<parameter name="ano" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT EXTRACT(MONTH FROM lf.lan_data_comp) AS mes,

       COALESCE(SUM(lf.lan_valor), 0)       AS total_mensal
FROM lancamento_financeiro lf
         INNER JOIN plano_de_contas pdc ON lf.plano_contas_id = pdc.id
WHERE pdc.natureza_conta = 'RECEITA'
  AND EXTRACT(YEAR FROM lf.lan_data_comp) =  $P{ano} 
GROUP BY mes
ORDER BY mes ASC]]></query>
	<field name="mes" class="java.math.BigDecimal">
		<property name="com.jaspersoft.studio.field.name" value="mes"/>
		<property name="com.jaspersoft.studio.field.label" value="mes"/>

	<query language="SQL"><![CDATA[select serv.serv_id,
       serv.serv_desc,
       serv.serv_preco,
       ost.ost_valor_unitario
from servico serv
         left join ordem_servico_item ost on serv.serv_id = ost.ost_servico_id
         left join ordem_servico os on ost.ost_ordem_servico_id = os.os_id
